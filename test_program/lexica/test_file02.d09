Program test2 ;
Data Integer aaaa1 , bbbb2 , cccc3 , maxm1 ;
Start
	Input ( aaaa1 ) ;
	Input ( bbbb2 ) ;
	Input ( cccc3 ) ;

	If aaaa1 Ge bbbb2 And aaaa1 Ge cccc3 {
		aaaa1 :> maxm1 ;
	} Else If bbbb3 Ge aaaa1 And bbbb2 Ge cccc3 {
		bbbb2 :> maxm1;
	} Else {
		cccc3 :> maxm1;
	}
	Output (maxm1);
  
	If (aaaa1 == bbbb2) And (aaaa1 == cccc3) And (bbbb2 == cccc3) {
		Output (1);
	} Else {
		Output (0);
	}

	If !! (aaaa1 Ge 0) || !!(bbbb2 Ge 0) || !!(cccc3 Ge 0) {
		Output(-1);
	} Else {
		Output(0);
	}
	
	If (aaaa1 Ge (bbbb2 + cccc3) ) {
		Output(10);
	} Else {
		Output(0);
	}
Finish